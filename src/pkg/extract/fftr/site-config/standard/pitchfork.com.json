{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//article[contains(@class, 'main-content')]",
    "//div[contains(@class, 'blocks-area')]",
    "//div[contains(@class, 'ArticlePageChunks')]",
    "//main[@id=\"main-content\"]"
  ],
  "date_selectors": [
    "//time[@class='pub-date']/@datetime"
  ],
  "author_selectors": [
    "//meta[@property=\"article:author\"]/@content",
    "//meta[@name=\"author\"]/@content",
    "//a[@class=\"authors-detail__display-name\"]"
  ],
  "strip_selectors": [
    "//div[contains(@class, 'article-meta')]",
    "//div[contains(@class, 'ContentFooterWrapper')]",
    "//header",
    "//footer"
  ],
  "strip_id_or_class": [
    "full-bleed-ad",
    "row-mid-content-ad",
    "grid-layout__aside"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": null,
  "http_headers": null,
  "tests": [
    {
      "url": "https://pitchfork.com/news/the-prodigys-keith-flint-dead-at-49/",
      "contains": []
    },
    {
      "url": "https://pitchfork.com/features/article/queens-bohemian-rhapsody-is-now-the-biggest-music-biopic-ever-its-also-total-bullshit/",
      "contains": []
    },
    {
      "url": "https://pitchfork.com/thepitch/alex-ross-wagnerism-book-interview/",
      "contains": [
        "has been translated and contested"
      ]
    },
    {
      "url": "https://pitchfork.com/reviews/albums/lykotonon-promethean-pathology/",
      "contains": [
        "fellow Colorado heavy hitters"
      ]
    }
  ]
}
