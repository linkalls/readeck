{
  "title_selectors": [
    "//h1[@class='headline']"
  ],
  "body_selectors": [
    "//div[@id='storyText']",
    "//img[@id='ff-img'] | //div[@id='meta']//div[contains(@class, 'overview')]"
  ],
  "date_selectors": [
    "//div[@class='date']"
  ],
  "author_selectors": [
    "//div[@class='byline']"
  ],
  "strip_selectors": [
    "//*[@id='date_partner']",
    "//div[@class='breadcrumb']",
    "//div[contains(@class,'show_tools')]",
