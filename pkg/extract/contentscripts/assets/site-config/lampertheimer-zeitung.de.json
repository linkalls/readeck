{
  "title_selectors": [
    "normalize-space(//meta[@property='og:title']/@content)"
  ],
  "body_selectors": [
    "//article"
  ],
  "date_selectors": [
    "//time/@datetime"
  ],
  "author_selectors": [
    "//meta[@name='twitter:creator']/@content"
  ],
  "strip_selectors": [
    "//path",
    "//script",
    "//footer",
    "//div[contains(@class, 'recommendations')]",
    "//div[@data-testid='storyline-intro-authors']",
    "//aside[contains(@data-testid, 'linkbox')]",
    "//svg"
  ],
  "strip_id_or_class": [
    "recommendations__topStories",
    "recommendations__cceWidget",
    "storyElementWrapper__paywallContainer",
    "storylineIntro__dateAndReadingTime",
    "loadingBanner",
    "adSlot",
    "storylineHeader"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": [
    [
      ",\"value\":",
      ",\"invalid\":"
    ],
    [
      ",\"storylineText\":",
      ",\"value\":"
    ],
    [
      "class=\"contentWrapper slider",
      "class=\"contentWrapper foo"
    ]
  ],
  "http_headers": {
    "User-Agent": "Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)",
    "referer": "https://www.echo-online.de/"
  },
  "tests": [
    {
      "url": "https://www.lampertheimer-zeitung.de/rss/lokales/hessen/",
      "contains": []
    }
  ]
}
