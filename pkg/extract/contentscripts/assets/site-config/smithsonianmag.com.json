{
  "title_selectors": [
    "//h1[@id = 'articleTitle']"
  ],
  "body_selectors": [
    "//div[@id = 'article-body']"
  ],
  "date_selectors": [
    "substring-before(substring-after(//ul[@id = 'byLine']/li[last()],','),',')"
  ],
  "author_selectors": [
    "substring-after(//ul[@id = 'byLine']/li[1],'By ')"
  ],
  "strip_selectors": [
    "//p[@id = 'articlePaginationWrapper']",
    "//ul[contains(@class, 'cat-breadcrumb')]",
    "//div [@class= 'viewMorePhotos']"
  ],
  "strip_id_or_class": null,
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": [
    "//td/li[@class = 'article-singlepage']/a",
    "//a[@id='continue-btn']"
  ],
  "next_page_link_selectors": null,
  "replace_strings": null,
  "http_headers": null,
  "tests": [
    {
      "url": "http://www.smithsonianmag.com/history-archaeology/The-Goddess-Goes-Home.html",
      "contains": []
