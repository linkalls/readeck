{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//article[1]"
  ],
  "date_selectors": [
    "//meta[@name=\"iso-8601-publish-date\"]/@content",
    "//meta[@name=\"parsely-pub-date\"]/@content",
    "//*[@itemprop='datePublished']/@datetime"
  ],
  "author_selectors": [
    "//meta[@name=\"sailthru.author\"]/@content",
    "//meta[@name=\"parsely-author\"]/@content"
  ],
  "strip_selectors": [
    "//div[contains(@class, 'Byline_bylineWrapper')]",
    "//div[contains(@class, 'gridLayout_topContent_')]",
    "//div[contains(@class, 'gridLayout_leftContent_')]",
    "//div[contains(@class, 'gridLayout_rightContent_')]",
    "//div[contains(@class, 'styles_bookmarkSpeech_')]",
    "//div[contains(@class, 'Image_imageOverlay')]",
    "//div[contains(@class, 'loatingCopyLink')]",
    "//div[contains(@class, 'AuthorBio_')]",
    "//div[contains(@class, 'styles_bookmarkBottom_')]",
    "//div[contains(@class, 'styles_shareFeedbackWrapper_')]",
    "//div[contains(@class, 'styles_toutWrapper_')]",
    "//div[contains(@class, 'InlineRecirc_')]",
    "//h3[contains(text(), 'ore from Bloomberg')] | //h3[contains(text(), 'ore from Bloomberg')]/following-sibling::*",
    "//aside"
  ],
  "strip_id_or_class": [
    "inline-newsletter",
    "terminal-tout-container",
    "lede",
    "postr-recirc",
    "left-column",
    "touts-v2",
    "eyebrow",
    "social-share",
    "icon-button",
    "outlined-button",
    "first-paragraph-image__credit"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": null,
  "http_headers": null,
  "tests": [
    {
      "url": "http://www.bloomberg.com/news/articles/2015-12-30/will-your-job-disappear-by-2024-",
      "contains": [
        "Every week, hosts Tori Stilwell"
      ]
    },
    {
      "url": "https://www.bloomberg.com/news/articles/2016-12-06/apple-to-start-publishing-ai-research-to-hasten-deep-learning",
      "contains": []
    }
  ]
}
