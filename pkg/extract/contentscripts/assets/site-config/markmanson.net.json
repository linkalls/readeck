{
  "title_selectors": [
    "//h1[contains(concat(' ',normalize-space(@class),' '),' entry-title ')]"
  ],
  "body_selectors": [
    "//article//div[contains(concat(' ',normalize-space(@class),' '),' entry-content ')]"
  ],
  "date_selectors": [
    "//meta[@property=\"article:published_time\"]/@content"
  ],
  "author_selectors": [
    "//meta[@property=\"article:author\"]/@content"
  ],
  "strip_selectors": [
    "//img[@data-lazy-src]"
  ],
  "strip_id_or_class": [
    "article-audio-player",
    "in-article-opt-in",
    "footer-opt-in",
    "no-print",
    "lazyloaded"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": [
    [
      "\u003cnoscript\u003e",
      "\u003cdiv\u003e"
    ],
    [
      "\u003c/noscript\u003e",
      "\u003c/div\u003e"
    ]
  ],
  "http_headers": null,
  "tests": [
    {
      "url": "https://markmanson.net/how-to-be-insanely-successful",
      "contains": [
        "you see everyone around you obsessed"
      ]
    },
    {
      "url": "https://markmanson.net/feed",
      "contains": []
    }
  ]
}
