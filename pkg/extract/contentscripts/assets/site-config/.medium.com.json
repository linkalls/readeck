{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content",
    "//h1"
  ],
  "body_selectors": [
    "//article"
  ],
  "date_selectors": [
    "//meta[@property=\"article:published_time\"]/@content"
  ],
  "author_selectors": [
    "//meta[@name=\"author\"]/@content"
  ],
  "strip_selectors": [
    "//button",
    "//svg",
    "//article//img[contains(@src, '?q=20') or contains(@src, 'max/34/')]",
    "//article//img[not(@src)]",
    "//header[contains(@class, 'pw-post-byline-header')]",
    "//h1[contains(@class, 'pw-post-title')]",
    "//a[@rel='noopener  ugc nofollow']/parent::div",
    "//footer/self::footer | //footer/following-sibling::*",
    "//article//img[@src]/following-sibling::*[1][self::div and @class='ftr-noscript']"
  ],
  "strip_id_or_class": [
    "speechify-ignore"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": [
    [
      "\u003cnoscript\u003e",
      "\u003cdiv class=\"ftr-noscript\"\u003e"
    ],
    [
      "\u003c/noscript\u003e",
      "\u003c/div\u003e"
    ]
  ],
  "http_headers": {
    "user-agent": "Mozilla/5.0 (compatible; Yahoo! Slurp; http://help.yahoo.com/help/us/ysearch/slurp)"
  },
  "tests": [
    {
      "url": "https://dougshapiro.medium.com/how-will-the-disruption-of-hollywood-play-out-42f724c921e1",
      "contains": [
        "you consider the costs for talent"
      ]
    },
    {
      "url": "https://elemental.medium.com/the-dark-side-of-fitness-tracking-9b218989bc47",
      "contains": [
        "Apps have turned movement and mindfulness"
      ]
    }
  ]
}
