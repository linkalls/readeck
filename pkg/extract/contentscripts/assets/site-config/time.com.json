{
  "title_selectors": [
    "//h1[contains(@class, 'article-title')]"
  ],
  "body_selectors": [
    "//div[@id=\"article-body\"]",
    "//section[contains(@class, 'article-body')]"
  ],
  "date_selectors": [
    "//div[@id=\"page-gtm-values\"]/div[contains(@class, 'keyvals')]/@data-content_published_date",
    "//time[@pubdate]/@datetime"
  ],
  "author_selectors": [
    "//div[@id=\"page-gtm-values\"]/div[contains(@class, 'keyvals')]/@data-content_author_name",
    "//article//span[contains(@class, 'byline')]"
  ],
  "strip_selectors": [
    "//figcaption",
    "//p[starts-with(., 'Read More:') and ./a]",
    "//aside"
  ],
  "strip_id_or_class": [
    "jumpstart-video",
    "inline-recirc",
    "most-popular-feed",
    "newsletter-callout",
    "author-feedback-text",
    "article-bottom",
    "video-jw",
    "ad-container"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": [
    [
      "data-src=\"",
      "data-src-disabled=\""
    ],
    [
      "\u003cnoscript\u003e",
      "\u003c!-- noscript --\u003e"
    ],
    [
      "\u003c/noscript\u003e",
      "\u003c!-- /noscript --\u003e"
    ]
  ],
  "http_headers": {
    "Cookie": "euConsent=true"
  },
  "tests": [
    {
      "url": "http://time.com/14478/emotions-may-not-be-so-universal-after-all/",
      "contains": []
    }
  ]
