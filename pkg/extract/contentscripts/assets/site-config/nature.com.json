{
  "title_selectors": [
    "//meta[@name=\"dc.title\"]/@content",
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//div[contains(concat(' ',normalize-space(@class),' '),' article__body ')] | //div[contains(concat(' ',normalize-space(@class),' '),' article-body ')] | //div[contains(@class, 'c-article-body')]"
  ],
  "date_selectors": [
    "//meta[@name=\"dc.date\"]/@content",
    "//meta[@name=\"prism.publicationDate\"]/@content"
  ],
  "author_selectors": [
    "//meta[@name='dc.creator']/@content"
  ],
  "strip_selectors": [
    "//div[contains(concat(' ',normalize-space(@id),' '),' further-reading-section ')]",
    "//noscript"
  ],
  "strip_id_or_class": null,
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": [
    [
      "data-src=\"",
      "src=\""
    ]
  ],
  "http_headers": {
    "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:137.0) Gecko/20100101 Firefox/137.0"
  },
  "tests": [
    {
      "url": "https://www.nature.com/articles/d41586-019-01307-2",
      "contains": []
    },
    {
      "url": "https://www.nature.com/npp/journal/v42/n11/full/npp201786a.html",
      "contains": []
    },
    {
      "url": "https://www.nature.com/articles/d41586-020-03050-5",
      "contains": [
        "A precociously gifted child"
      ]
    }
  ]
}
