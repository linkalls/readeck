{
  "title_selectors": [
    "//h2[contains(@class, 'primary')]"
  ],
  "body_selectors": [
    "//*[@itemprop=\"articleBody\"]",
    "//div[@id='story']"
  ],
  "date_selectors": [
    "substring-after(//*[@class='date'], 'Published')"
  ],
  "author_selectors": [
    "//*[@class='by']/a"
  ],
  "strip_selectors": null,
  "strip_id_or_class": [
    "fns-desktop"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": [
    "//div[@class='page-navigation']//li[@class='next']/a"
  ],
  "replace_strings": null,
  "http_headers": {
    "Cookie": "nymuc=11111111111"
  },
  "tests": [
    {
      "url": "http://nymag.com/news/features/wall-street-2012-2/",
      "contains": [
        "bonus season is a sacred ritual"
      ]
    },
    {
      "url": "http://nymag.com/daily/intelligencer/2016/04/america-tyranny-donald-trump.html",
      "contains": [
        "This rainbow-flag polity"
      ]
    },
    {
      "url": "https://nymag.com/intelligencer/2016/06/the-hack-that-could-take-down-nyc.html",
      "contains": [
        "Justice Department representative"
      ]
    }
  ]
}
