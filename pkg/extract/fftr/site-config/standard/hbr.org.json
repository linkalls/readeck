{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//div[contains(concat(' ',normalize-space(@class),' '),' article ')]"
  ],
  "date_selectors": [
    "//meta[@property=\"article:published_time\"]/@content"
  ],
  "author_selectors": [
    "//div[@class=\"article-byline detached\"]/ul[@class=\"article-byline-list\"]/li[@class=\"article-author\"]/a"
  ],
  "strip_selectors": [
    "//article-ideainbrief",
    "//div[@class=\"article-summary\"]/text()"
  ],
  "strip_id_or_class": [
    "hide-for-print",
    "is-hidden",
    "translate-message",
    "translate-message-narrow-art",
    "left-rail--container",
    "article-promo"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": [
    [
      "\u003carticle-promo class",
      "\u003cdiv class"
    ],
    [
      "\u003c/article-promo\u003e",
      "\u003c/div\u003e"
    ]
  ],
  "http_headers": null,
  "tests": [
    {
      "url": "http://hbr.org/2012/04/the-real-leadership-lessons-of-steve-jobs/ar/",
      "contains": []
    },
    {
      "url": "http://hbr.org/2013/03/big-bang-disruption/ar/",
      "contains": []
    },
    {
      "url": "https://hbr.org/2020/11/how-apple-is-organized-for-innovation",
      "contains": [
        "individual and team reputations act as a control mechanism"
      ]
    },
    {
      "url": "https://hbr.org/2015/09/to-become-a-leader-think-beyond-your-role",
      "contains": []
    },
    {
      "url": "https://hbr.org/2017/05/your-brain-can-only-take-so-much-focus",
      "contains": []
    }
  ]
}
