{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//h3[contains(concat(' ',normalize-space(@class),' '),' subtitle ')] | //div[contains(concat(' ',normalize-space(@class),' '),' body ')]"
  ],
  "date_selectors": null,
  "author_selectors": [
    "//meta[@name=\"author\"]/@content"
  ],
  "strip_selectors": [
    "//div[contains(@class, 'tweet-footer')]//span",
    "//p[contains(concat(' ',normalize-space(@class),' '),' button-wrapper ')]"
  ],
  "strip_id_or_class": [
    "expanded-link-description",
    "expanded-link-domain",
    "header-anchor-widget",
    "subscribe-widget"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
  "replace_strings": null,
  "http_headers": null,
  "tests": [
    {
      "url": "https://taibbi.substack.com/p/glenn-greenwald-on-his-resignation",
      "contains": [
        "Greenwald, by then furious, noted that neither Maass nor Reed had identified a factual inaccuracy"
      ]
    },
    {
      "url": "https://jonathancook.substack.com/p/why-the-western-media-is-afraid-of",
      "contains": [
        "The goal of the corporate media is not unearthing truth"
      ]
    }
  ]
}
