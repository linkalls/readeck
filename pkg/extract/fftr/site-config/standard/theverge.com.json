{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//div[contains(@class, 'c-entry-content') or contains(@class, 'c-entry-hero__image')]",
    "//article",
    "//div[contains(concat(' ',normalize-space(@class),' '),' l-col__main ')]"
  ],
  "date_selectors": [
    "//meta[@property=\"article:published_time\"]/@content"
  ],
  "author_selectors": [
    "//meta[@name=\"author\"]/@content"
