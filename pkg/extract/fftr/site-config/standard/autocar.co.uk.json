    "//div[@class='content'] | //div[@class='lower-content-region'] | //div[contains(@class,'article-body')] | //div[contains(@class,'main-article')]"
  ],
  "date_selectors": [
    "//div[@class='personality-date']"
  ],
  "author_selectors": [
    "//div[@class='personality']/a"
