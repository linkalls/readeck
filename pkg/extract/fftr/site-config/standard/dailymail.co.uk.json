    "articleIconLinksContainer",
    "related-carousel",
    "reader-comments",
    "most-watched",
    "most-read",
    "imageCaption"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
