{
  "title_selectors": [
    "//meta[@property=\"og:title\"]/@content"
  ],
  "body_selectors": [
    "//div[@id='js-article-text']"
  ],
  "date_selectors": [
    "//meta[@property=\"article:published_time\"]/@content"
  ],
  "author_selectors": [
    "//meta[@name=\"author\"]/@content"
  ],
  "strip_selectors": [
    "//div[@class='explore-links']",
    "//div[@id='js-article-text']/br[position()=1]",
