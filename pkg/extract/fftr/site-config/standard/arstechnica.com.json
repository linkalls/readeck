{
  "title_selectors": [
    "//div[@id='story']//h2[@class='title']"
  ],
  "body_selectors": [
    "//div[contains(@class,'article-content')]"
  ],
  "date_selectors": [
    "//div[@class='byline']/span[@class='posted']//abbr/@original-title",
    "//div[@class='byline']/span[@class='posted']//abbr",
    "//*[@class='byline']//time[@class='date']"
  ],
  "author_selectors": [
    "//p[@class='byline']/span[@class='author']",
    "//p[@class='byline']/a"
  ],
  "strip_selectors": [
    "//h2[@class='title']",
    "//div[contains(@class, 'pullbox')]",
    "//div[@class='pager']",
    "//aside"
  ],
  "strip_id_or_class": [
    "byline",
    "story-sidebar"
  ],
  "strip_image_src": null,
  "native_ad_selectors": [
    "//meta[@property=\"og:url\" and contains(@content, '/sponsored/')]"
  ],
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": [
    "//nav//a[contains(text(), 'Next')]/@href",
    "//span[@class='numbers']//a/span[@class='next']/..",
    "//nav//a/span[contains(text(), 'Next')]/../@href"
  ],
  "replace_strings": [
    [
      "\" data-src=\"",
      "\"\u003e\u003cimg src=\""
    ],
    [
      "\" data-responsive=\"",
      "\" /\u003e\u003cspan \""
    ],
    [
      "\u003cfigure style=\"",
      "\u003c/span\u003e\u003cfigure style=\""
    ]
  ],
  "http_headers": null,
